{
  "model_name": "deepseek-ai/DeepSeek-R1-Distill-Qwen-7B",
  "output_dir": "./model-finetuned-rtx4050",
  "data_dir": "./data",
  "max_seq_length": 1024,
  "validation_split": 0.05,
  "epochs": 3,
  "batch_size": 1,
  "gradient_accumulation_steps": 4,
  "learning_rate": 5e-05,
  "warmup_steps": 50,
  "weight_decay": 0.01,
  "logging_steps": 20,
  "eval_steps": 200,
  "save_steps": 800,
  "seed": 42,
  "use_lora": true,
  "lora_r": 4,
  "lora_alpha": 8,
  "lora_dropout": 0.05,
  "lora_target_modules": "q_proj,v_proj",
  "use_4bit": false,
  "use_8bit": false,
  "gradient_checkpointing": true
}